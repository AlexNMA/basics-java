<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane style="-fx-background-color: gray;"
	xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="javafxcontroller">
	<children>
		<Pane prefHeight="314.0" prefWidth="470.0">
			<children>
				<AnchorPane layoutX="9.0" layoutY="267.0"
					prefHeight="37.0" prefWidth="326.0">
					<children>
						<Button id="bt1" alignment="CENTER" layoutX="1.0"
							layoutY="3.0" mnemonicParsing="false" onAction="#btn1OKClicked"
							prefHeight="30.0" prefWidth="100.0"
							style="-fx-background-color: silver; -fx-alignment: CENTER;"
							text="ADD">
							<font>
								<Font name="Candara Bold" size="14.0" />
							</font>
						</Button>
						<Button id="bt2" alignment="CENTER" layoutX="122.0"
							layoutY="3.0" mnemonicParsing="false" onAction="#btn2OKClicked"
							prefHeight="30.0" prefWidth="100.0"
							style="-fx-background-color: silver; -fx-alignment: CENTER;"
							text="VIEW">
							<font>
								<Font name="Candara Bold" size="14.0" />
							</font>
						</Button>
						<Button id="bt3" alignment="CENTER" layoutX="239.0"
							layoutY="3.0" mnemonicParsing="false" onAction="#btn3OKClicked"
							prefHeight="30.0" prefWidth="100.0"
							style="-fx-background-color: silver; -fx-alignment: CENTER;"
							text="EDIT">
							<font>
								<Font name="Candara Bold" size="14.0" />
							</font>
						</Button>
						<Button id="bt4" alignment="CENTER" layoutX="353.0"
							layoutY="3.0" mnemonicParsing="false" onAction="#btn4OKClicked"
							prefHeight="30.0" prefWidth="100.0"
							style="-fx-background-color: silver; -fx-alignment: CENTER;"
							text="DELETE">
							<font>
								<Font name="Candara Bold" size="14.0" />
							</font>
						</Button>
					</children>
				</AnchorPane>
				<AnchorPane layoutX="15.0" layoutY="12.0"
					prefHeight="245.0" prefWidth="446.0">
					<children>
						<TableView fx:id="tableView" layoutY="5.0"
							prefHeight="245.0" prefWidth="446.0"
							style="-fx-background-color: gray;" AnchorPane.bottomAnchor="0.0"
							AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
							AnchorPane.topAnchor="0.0">
							<columns>
							<TableColumn prefWidth="37.0" text="ID"
									fx:id="tableColumnID"/>
								<TableColumn prefWidth="329.0" text="Name"
									fx:id="tableColumnName"/>
								<TableColumn prefWidth="77.0" text="Age"
									fx:id="tableColumnAge"/>
							</columns>
						</TableView>
					</children>
				</AnchorPane>
			</children>
		</Pane>
	</children>
</AnchorPane>
